{
  "manifest_version": 3,
  "name": "Touch",
  "version": "0.1.0",
  "action": {
    "default_title": "Save to Touch"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["storage", "scripting", "activeTab", "tabs", "notifications"],
  "host_permissions": [
    "http://localhost:3012/*",
    "https://touch.starmode.dev/*"
  ]
}
