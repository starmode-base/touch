{
  "name": "touch",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": "22.x",
    "bun": "1.3.1"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "prettier --check . && eslint --max-warnings 0 && vite build && tsc --noEmit && drizzle-kit migrate && vitest run",
    "start": "PORT=$(scripts/start.ts) node -r dotenv/config .output/server/index.mjs",
    "format": "prettier --write .",
    "lint": "eslint --max-warnings 0 --fix",
    "test": "vitest",
    "env:pull": "bunx vercel@latest env pull .env",
    "clean:soft": "rm -rf node_modules tsconfig.tsbuildinfo .nitro .output .tanstack && bun install",
    "clean:hard": "rm -rf node_modules tsconfig.tsbuildinfo .nitro .output .tanstack bun.lock && bun install",
    "db:generate": "drizzle-kit generate",
    "db:generate:custom": "drizzle-kit generate --custom",
    "db:drop": "drizzle-kit drop",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@clerk/tanstack-react-start": "^0.26.4",
    "@electric-sql/client": "^1.0.14",
    "@heroicons/react": "^2.2.0",
    "@neondatabase/serverless": "^1.0.2",
    "@tailwindcss/vite": "^4.1.16",
    "@tanstack/electric-db-collection": "^0.1.36",
    "@tanstack/query-core": "^5.90.5",
    "@tanstack/query-db-collection": "^0.2.33",
    "@tanstack/react-db": "^0.1.34",
    "@tanstack/react-router": "^1.133.22",
    "@tanstack/react-router-devtools": "^1.133.22",
    "@tanstack/react-start": "^1.133.22",
    "@vercel/analytics": "^1.5.0",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "nanoid": "^5.1.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "srvx": "0.8.15",
    "tailwindcss": "^4.1.16",
    "tiny-invariant": "^1.3.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@tanstack/eslint-plugin-router": "^1.133.19",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.38.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "globals": "^16.4.0",
    "neon-testing": "^2.1.4",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "sharp": "^0.34.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
