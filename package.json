{
  "name": "touch",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "prettier --check . && eslint --max-warnings 0 && vite build && tsc --noEmit && drizzle-kit migrate && vitest run",
    "start": "PORT=$(scripts/start.ts) node -r dotenv/config .output/server/index.mjs",
    "format": "prettier --write .",
    "lint": "eslint --max-warnings 0 --fix",
    "test": "vitest",
    "env:pull": "bunx vercel@latest env pull .env",
    "clean:soft": "rm -rf node_modules tsconfig.tsbuildinfo .nitro .output .tanstack dist && bun install",
    "clean:hard": "rm -rf node_modules tsconfig.tsbuildinfo .nitro .output .tanstack dist bun.lock && bun install",
    "db:generate": "drizzle-kit generate",
    "db:generate:custom": "drizzle-kit generate --custom",
    "db:drop": "drizzle-kit drop",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@clerk/tanstack-react-start": "^0.27.5",
    "@electric-sql/client": "^1.2.0",
    "@heroicons/react": "^2.2.0",
    "@neondatabase/serverless": "^1.0.2",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/electric-db-collection": "^0.2.9",
    "@tanstack/react-db": "^0.1.52",
    "@tanstack/react-router": "1.133.36",
    "@tanstack/react-router-devtools": "1.133.36",
    "@tanstack/react-start": "1.134.0",
    "@vercel/analytics": "^1.5.0",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.3",
    "nanoid": "^5.1.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "srvx": "0.8.15",
    "tailwindcss": "^4.1.17",
    "tiny-invariant": "^1.3.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tanstack/eslint-plugin-router": "^1.139.0",
    "@tanstack/nitro-v2-vite-plugin": "^1.139.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "neon-testing": "^2.3.1",
    "prettier": "^3.7.3",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "sharp": "^0.34.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.14"
  }
}
