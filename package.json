{
  "name": "touch",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev:cf": "wrangler dev",
    "deploy:cf": "wrangler deploy",
    "dev": "vite dev",
    "build": "prettier --check . && eslint --max-warnings 0 && vite build && tsc --noEmit && drizzle-kit migrate && vitest run",
    "start_": "PORT=$(scripts/start.ts) node -r dotenv/config .output/server/index.mjs",
    "preview": "vite preview",
    "format": "prettier --write .",
    "cf-typegen": "wrangler types && prettier --write worker-configuration.d.ts",
    "lint": "eslint --max-warnings 0 --fix",
    "test": "vitest",
    "env:pull": "bunx vercel@latest env pull .env",
    "clean:soft": "rm -rf node_modules tsconfig.tsbuildinfo .nitro .output .tanstack dist .wrangler && bun install",
    "clean:hard": "rm -rf node_modules tsconfig.tsbuildinfo .nitro .output .tanstack dist .wrangler bun.lock && bun install",
    "db:generate": "drizzle-kit generate",
    "db:generate:custom": "drizzle-kit generate --custom",
    "db:drop": "drizzle-kit drop",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@electric-sql/client": "^1.2.0",
    "@heroicons/react": "^2.2.0",
    "@neondatabase/serverless": "^1.0.2",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/electric-db-collection": "^0.2.11",
    "@tanstack/react-db": "^0.1.54",
    "@tanstack/react-router": "^1.139.14",
    "@tanstack/react-router-devtools": "^1.139.14",
    "@tanstack/react-start": "^1.139.14",
    "@vercel/analytics": "^1.6.1",
    "better-auth": "^1.4.5",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "nanoid": "^5.1.6",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "tailwindcss": "^4.1.17",
    "tiny-invariant": "^1.3.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.25.0",
    "@eslint/js": "^9.39.2",
    "@tanstack/eslint-plugin-router": "^1.155.0",
    "@tanstack/nitro-v2-vite-plugin": "^1.154.7",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "dotenv": "^17.3.1",
    "drizzle-kit": "^0.31.9",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "neon-testing": "^2.5.0",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "sharp": "^0.34.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18",
    "wrangler": "^4.65.0"
  }
}
