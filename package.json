{
  "name": "touch",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "prettier --check . && eslint --max-warnings 0 && vite build && tsc --noEmit && drizzle-kit migrate && vitest run",
    "start": "PORT=$(scripts/start.ts) node -r dotenv/config .output/server/index.mjs",
    "format": "prettier --write .",
    "lint": "eslint --max-warnings 0 --fix",
    "test": "vitest",
    "env:pull": "bunx vercel@latest env pull .env",
    "clean:soft": "rm -rf node_modules tsconfig.tsbuildinfo .nitro .output .tanstack && bun install",
    "clean:hard": "rm -rf node_modules tsconfig.tsbuildinfo .nitro .output .tanstack bun.lock && bun install",
    "db:generate": "drizzle-kit generate",
    "db:generate:custom": "drizzle-kit generate --custom",
    "db:drop": "drizzle-kit drop",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@clerk/tanstack-react-start": "^0.25.0",
    "@heroicons/react": "^2.2.0",
    "@neondatabase/serverless": "^1.0.1",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/electric-db-collection": "^0.1.25",
    "@tanstack/query-db-collection": "^0.2.22",
    "@tanstack/react-db": "^0.1.23",
    "@tanstack/react-router": "~1.131.50",
    "@tanstack/react-router-devtools": "~1.131.50",
    "@tanstack/react-start": "~1.131.50",
    "@vercel/analytics": "^1.5.0",
    "drizzle-orm": "^0.44.5",
    "drizzle-zod": "^0.8.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.13",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tanstack/eslint-plugin-router": "~1.131.2",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.4.0",
    "neon-testing": "^2.1.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sharp": "^0.34.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
